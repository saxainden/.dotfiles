
GIT_PS1_SHOWDIRTYSTATE=1

source ~/.git-prompt.sh
RED='\e[0;91m'         # Red
GREEN='\e[0;92m'       # Green
YELLOW='\e[0;93m'      # Yellow
BLUE='\e[0;94m'        # Blue
PURPLE='\e[0;95m'      # Purple
CYAN='\e[0;96m'        # Cyan
WHITE='\e[0;97m'       # White

PS1="\[$GREEN\]\w\[\033[m\]\[$CYAN\]\$(__git_ps1)\[$WHITE\]: "
